<p id="notice"><%= notice %></p>

<h1>Orders</h1>

<table>
  <thead>
    <tr>
      <th>Steam</th>
      <th>Status</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @orders.each do |order| %>
      <tr>
        <td><%= order.steam_id %></td>
        <td><%= order.status %></td>
        <td><%= order.user_id %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>